{
    "rules": {
        /**
         * @description: Helpful warnings
         * @see https://github.com/import-js/eslint-plugin-import/tree/main?tab=readme-ov-file#helpful-warnings
         * @version 2.29.1
         */


        "import/export": "error",
        "import/no-deprecated": "warn",
        "import/no-empty-named-blocks": "warn",
        "import/no-extraneous-dependencies": "warn",
        "import/no-mutable-exports": "warn",
        "import/no-named-as-default": "warn",
        "import/no-named-as-default-member": "warn",
        "import/no-unused-modules": "warn",


        /**
         * @description: Module systems
         * @see https://github.com/import-js/eslint-plugin-import/tree/main?tab=readme-ov-file#module-systems
         * @version 2.29.1
         */

        "import/no-amd": "error",
        "import/no-commonjs": "error",
        "import/no-import-module-exports": "error",
        "import/no-nodejs-modules": "error",
        "import/unambiguous": "error",


        /**
         * @description: Static analysis
         * @see https://github.com/import-js/eslint-plugin-import/tree/main?tab=readme-ov-file#static-analysis
         * @version 2.29.1
         */

        "import/default": "error",
        "import/named": "error",
        "import/namespace": "error",
        "import/no-absolute-path": "error",
        "import/no-cycle": "error",
        "import/no-dynamic-require": "warn",
        // "import/no-internal-modules": "warn",
        "import/no-relative-packages": "warn",
        // "import/no-relative-parent-imports": "warn",
        // "import/no-restricted-paths": "warn",
        "import/no-self-import": "warn",
        "import/no-unresolved": "error",
        "import/no-useless-path-segments": "warn",
        "import/no-webpack-loader-syntax": "error",

        /**
         * @description: Style guide
         * @see https://github.com/import-js/eslint-plugin-import/tree/main?tab=readme-ov-file#style-guide
         * @version 2.29.1
         */

        "import/consistent-type-specifier-style": ["warn", "prefer-top-level"],
        // "import/dynamic-import-chunkname": "warn",
        // "import/exports-last": "warn",
        "import/extensions": "warn",
        "import/first": "warn",
        // "import/group-exports": "warn",
        // "import/imports-first": "warn", // DEPRECATED
        // "import/max-dependencies": "warn",
        "import/newline-after-import": ["warn", { "count": 2 }],
        // "import/no-anonymous-default-export": "warn",
        // "import/no-default-export": "warn",
        "import/no-duplicates": "warn",
        "import/no-named-default": "warn",
        // "import/no-namespace": "warn",
        // "import/no-unassigned-import": "warn",
        // "import/order": "warn",
        // "import/prefer-default-export": "warn",


        /**
         * last
         * @description: dummy rule, to avoid dealing with trailing commas above
         */
        "": "off"
    }
}
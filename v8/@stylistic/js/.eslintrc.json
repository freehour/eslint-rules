{
    "rules": {
        /**
         * @description: eslint stylistic rules
         * @see https://eslint.style/packages/js#rules
         * @version 2.1.0
         */

        "@stylistic/js/array-bracket-newline": "warn",
        "@stylistic/js/array-bracket-spacing": "warn",
        "@stylistic/js/array-element-newline": [
            "warn",
            "consistent"
        ],
        "@stylistic/js/arrow-parens": [
            "warn",
            "as-needed"
        ],
        "@stylistic/js/arrow-spacing": "warn",
        // "@stylistic/js/block-spacing": "warn", EXTENDED,
        // "@stylistic/js/brace-style": "warn", EXTENDED,
        // "@stylistic/js/comma-dangle": "warn", EXTENDED,
        // "@stylistic/js/comma-spacing": "warn", EXTENDED,
        "@stylistic/js/comma-style": "warn",
        "@stylistic/js/computed-property-spacing": "warn",
        "@stylistic/js/dot-location": [
            "warn",
            "property"
        ],
        "@stylistic/js/eol-last": "warn",
        "@stylistic/js/function-call-argument-newline": [
            "warn",
            "consistent"
        ],
        // "@stylistic/js/func-call-spacing": "warn", EXTENDED // DEPRECATED,
        // "@stylistic/js/function-call-spacing": "warn", EXTENDED,
        "@stylistic/js/function-paren-newline": [
            "warn",
            "multiline-arguments"
        ],
        // "@stylistic/js/generator-star-spacing": "warn",
        "@stylistic/js/implicit-arrow-linebreak": "warn",
        // "@stylistic/js/indent": "warn", EXTENDED,
        "@stylistic/js/jsx-quotes": "warn",
        // "@stylistic/js/key-spacing": "warn" EXTENDED,
        // "@stylistic/js/keyword-spacing": "warn" EXTENDED,
        // "@stylistic/js/line-comment-position": "warn",
        "@stylistic/js/linebreak-style": "warn",
        // "@stylistic/js/lines-around-comment": "warn", EXTENDED,
        // "@stylistic/js/lines-between-class-members": "warn", EXTENDED,
        // "@stylistic/js/max-len": "warn",
        "@stylistic/js/max-statements-per-line": "warn",
        // "@stylistic/js/multiline-comment-style": "warn",
        "@stylistic/js/multiline-ternary": [
            "warn",
            "always-multiline"
        ],
        "@stylistic/js/new-parens": "warn",
        "@stylistic/js/newline-per-chained-call": "warn",
        // "@stylistic/js/no-confusing-arrow": "warn",
        // "@stylistic/js/no-extra-parens": "warn", EXTENDED
        // "@stylistic/js/no-extra-semi": "warn", EXTENDED
        "@stylistic/js/no-floating-decimal": "warn",
        "@stylistic/js/no-mixed-operators": "warn",
        "@stylistic/js/no-mixed-spaces-and-tabs": "error",
        "@stylistic/js/no-multi-spaces": [
            "warn",
            {
                "ignoreEOLComments": true
            }
        ],
        "@stylistic/js/no-multiple-empty-lines": [
            "warn",
            {
                "max": 2,
                "maxEOF": 1
            }
        ],
        "@stylistic/js/no-tabs": "error",
        "@stylistic/js/no-trailing-spaces": "warn",
        "@stylistic/js/no-whitespace-before-property": "warn",
        // "@stylistic/js/nonblock-statement-body-position": "warn",
        "@stylistic/js/object-curly-newline": [
            "warn",
            {
                "multiline": true
            }
        ],
        // "@stylistic/js/object-curly-spacing": "warn", EXTENDED
        "@stylistic/js/object-property-newline": [
            "warn",
            {
                "allowAllPropertiesOnSameLine": true
            }
        ],
        "@stylistic/js/one-var-declaration-per-line": "warn",
        "@stylistic/js/operator-linebreak": [
            "warn",
            "before"
        ],
        "@stylistic/js/padded-blocks": [
            "warn",
            "never"
        ],
        // "@stylistic/js/padding-line-between-statements": "warn", EXTENDED
        // "@stylistic/js/quote-props": "warn", EXTENDED
        // "@stylistic/js/quotes": "warn", EXTENDED
        "@stylistic/js/rest-spread-spacing": "warn",
        // "@stylistic/js/semi": "warn", EXTENDED
        "@stylistic/js/semi-spacing": "warn",
        "@stylistic/js/semi-style": "warn",
        // "@stylistic/js/space-before-blocks": "warn", EXTENDED
        // "@stylistic/js/space-before-function-paren": "warn", EXTENDED
        "@stylistic/js/space-in-parens": "warn",
        // "@stylistic/js/space-infix-ops": "warn", EXTENDED
        "@stylistic/js/space-unary-ops": "warn",
        "@stylistic/js/spaced-comment": "warn",
        "@stylistic/js/switch-colon-spacing": "warn",
        "@stylistic/js/template-curly-spacing": "warn",
        "@stylistic/js/template-tag-spacing": "warn",
        "@stylistic/js/wrap-iife": "warn",
        // "@stylistic/js/wrap-regex": "warn",
        // "@stylistic/js/yield-star-spacing": "warn",


        /**
         * last
         * @description: dummy rule, to avoid dealing with trailing commas above
         */

         "": "off"
    }
}